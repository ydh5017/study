                                    <h1 class="h3 mb-0 text-gray-800">내가 작성한 댓글</h1>
                                </div>
                            </div>
                            <div class="text-xl text-right font-weight-bold text-uppercase mb-3 mr-4">
                                <a href="/user/comment?mypageType=writeComment&pno=1" id="href">더보기</a>
                                <i class="fas fa-angle-right"></i>
                            </div>
                        </div>
                        {{#writeCommentList}}
                        <div class="card-body border-bottom p-0">
                            <div class="row no-gutters align-items-center">
                                <div class="col mr-2">
                                    <div class="mb-1">
                                        <div class="text-xl font-weight-bold text-uppercase" style="float: left">
                                            댓글이 달린 게시글 : <a class="text-primary" onclick="return movePostDetail({{postSeq}}, {{commentSeq}})">"{{postTitle}}"</a>
                                            <div class="h5 mb-0 font-weight-bold text-gray-800" style="">
                                                <input type="hidden" id="commentContent-" value="content">
                                                {{content}}
                                            </div>
                                        </div>
                                        <div class="text-xs text-right font-weight-bold text-uppercase" style="float: right">
