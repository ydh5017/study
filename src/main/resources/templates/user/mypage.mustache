                                                            <input type="hidden" name="commentSeq" value="{{commentSeq}}">
                                                            <input type="hidden" name="postSeq" value="{{postSeq}}">
                                                            <input type="hidden" name="location" value="mypage">
                                                            <button class="dropdown-item" type="submit">삭제</button>
                                                        </form>
                                                    </div>
                                                </nav>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/writeCommentList}}
